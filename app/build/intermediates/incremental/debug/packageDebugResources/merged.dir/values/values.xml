<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#FF000000</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="app_name">Bkash Payment</string>
    <string name="title_dashboard">Search trxID</string>
    <string name="title_home">bKash Pay</string>
    <string name="title_notifications">Notifications</string>
    <style name="DialogStyle" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorPrimary">#F50057</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">#FFFFFF</item>
        <item name="android:indeterminateTint">#F50057</item>
    </style>
    <style name="FullScreenDialogStyle" parent="Theme.BKashTokenizedRnD">
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
    </style>
    <style name="SheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.3</item>
        <item name="android:windowFrame">@null</item>
        <item name="behavior_peekHeight">300dp</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>
    <style name="Theme.BKashTokenizedRnD" parent="Theme.AppCompat.Light.NoActionBar">
        
        <item name="colorPrimary">#E91E63</item>
        <item name="colorPrimaryDark">#E91E63</item>
        <item name="colorAccent">#E91E63</item>
        
    </style>
</resources>